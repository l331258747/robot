<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <androidx.constraintlayout.widget.Guideline
        android:id="@+id/line_start_40"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        app:layout_constraintGuide_begin="@dimen/dp_40" />

    <View
        android:id="@+id/line_center"
        android:layout_width="1dp"
        android:layout_height="1dp"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <TextView
        android:id="@+id/tv_qzsxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="前置摄像头"
        android:layout_marginTop="@dimen/dp_40"
        android:textColor="@color/white"
        app:layout_constraintStart_toStartOf="@+id/line_start_40"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_qztz"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_40"
        android:text="前置+态资"
        android:textColor="@color/white"
        app:layout_constraintStart_toStartOf="@+id/line_start_40"
        app:layout_constraintTop_toBottomOf="@+id/tv_qzsxt" />

    <TextView
        android:id="@+id/tv_qztzmbsb"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/dp_40"
        android:text="前置+态资+目标识别"
        android:textColor="@color/white"
        app:layout_constraintStart_toStartOf="@+id/line_start_40"
        app:layout_constraintTop_toBottomOf="@+id/tv_qztz" />

    <TextView
        android:id="@+id/tv_cssxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="侧视摄像头"
        android:textColor="@color/white"
        android:layout_marginTop="@dimen/dp_40"
        app:layout_constraintStart_toStartOf="@+id/line_center"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/tv_hssxt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="后视摄像头"
        android:textColor="@color/white"
        android:layout_marginTop="@dimen/dp_40"
        app:layout_constraintStart_toStartOf="@+id/line_center"
        app:layout_constraintTop_toBottomOf="@+id/tv_cssxt" />

</androidx.constraintlayout.widget.ConstraintLayout>