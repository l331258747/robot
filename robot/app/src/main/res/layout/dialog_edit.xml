<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/layout_parent"
    android:layout_width="@dimen/dp_320"
    android:layout_height="wrap_content"
    android:background="@drawable/shape_00_664b4b4b_r5"
    android:orientation="vertical">


    <TextView
        android:id="@+id/tv_title"
        android:layout_width="@dimen/dp_320"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:layout_marginTop="@dimen/dp_10"
        android:includeFontPadding="false"
        android:text="添加无人车"
        android:textSize="@dimen/sp_20"
        android:textColor="@color/color_1e1e1e"
        android:textStyle="bold"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintRight_toRightOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view_account"
        android:layout_width="0dp"
        android:layout_height="@dimen/dp_35"
        android:layout_marginStart="@dimen/dp_40"
        android:layout_marginTop="@dimen/dp_10"
        android:layout_marginEnd="@dimen/dp_40"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tv_title" />

    <TextView
        android:id="@+id/tv_account"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="IP："
        android:textColor="@color/color_1e1e1e"
        app:layout_constraintBottom_toBottomOf="@id/view_account"
        app:layout_constraintStart_toStartOf="@+id/view_account"
        app:layout_constraintTop_toTopOf="@+id/view_account" />

    <EditText
        android:id="@+id/et_account"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/shape_eeeeeed_r40"
        android:hint="请输入IP"
        android:maxLength="15"
        android:maxLines="1"
        android:layout_marginStart="@dimen/dp_56"
        android:paddingStart="@dimen/dp_10"
        android:paddingEnd="@dimen/dp_10"
        app:layout_constraintBottom_toBottomOf="@+id/view_account"
        app:layout_constraintEnd_toEndOf="@+id/view_account"
        app:layout_constraintStart_toStartOf="@+id/view_account"
        app:layout_constraintTop_toTopOf="@+id/view_account" />

    <View
        android:id="@+id/view_password"
        android:layout_width="0dp"
        android:layout_height="@dimen/dp_35"
        android:layout_marginStart="@dimen/dp_40"
        android:layout_marginTop="@dimen/dp_5"
        android:layout_marginEnd="@dimen/dp_40"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view_account" />

    <TextView
        android:id="@+id/tv_password"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="端口："
        android:textColor="@color/color_1e1e1e"
        app:layout_constraintBottom_toBottomOf="@id/view_password"
        app:layout_constraintStart_toStartOf="@+id/view_password"
        app:layout_constraintTop_toTopOf="@+id/view_password" />

    <EditText
        android:id="@+id/et_password"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/shape_eeeeeed_r40"
        android:hint="请输入端口"
        android:inputType="number"
        android:maxLength="6"
        android:textColor="@color/color_1e1e1e"
        android:layout_marginStart="@dimen/dp_56"
        android:maxLines="1"
        android:paddingStart="@dimen/dp_10"
        android:paddingEnd="@dimen/dp_10"
        app:layout_constraintBottom_toBottomOf="@+id/view_password"
        app:layout_constraintEnd_toEndOf="@+id/view_password"
        app:layout_constraintStart_toStartOf="@+id/view_password"
        app:layout_constraintTop_toTopOf="@+id/view_password" />

    <View
        android:id="@+id/view_password2"
        android:layout_width="0dp"
        android:layout_height="@dimen/dp_35"
        android:layout_marginStart="@dimen/dp_40"
        android:layout_marginTop="@dimen/dp_5"
        android:layout_marginEnd="@dimen/dp_40"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view_password" />


    <TextView
        android:id="@+id/tv_password2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="编号："
        android:textColor="@color/color_1e1e1e"
        app:layout_constraintBottom_toBottomOf="@id/view_password2"
        app:layout_constraintStart_toStartOf="@+id/view_password2"
        app:layout_constraintTop_toTopOf="@+id/view_password2" />

    <EditText
        android:id="@+id/et_password2"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:background="@drawable/shape_eeeeeed_r40"
        android:hint="请输入编号"
        android:inputType="number"
        android:maxLength="4"
        android:textColor="@color/color_1e1e1e"
        android:layout_marginStart="@dimen/dp_56"
        android:maxLines="1"
        android:paddingStart="@dimen/dp_10"
        android:paddingEnd="@dimen/dp_10"
        app:layout_constraintBottom_toBottomOf="@+id/view_password2"
        app:layout_constraintEnd_toEndOf="@+id/view_password2"
        app:layout_constraintStart_toStartOf="@+id/view_password2"
        app:layout_constraintTop_toTopOf="@+id/view_password2" />

    <View
        android:id="@+id/view_rtsp"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="@dimen/dp_40"
        android:layout_marginTop="@dimen/dp_5"
        android:layout_marginEnd="@dimen/dp_40"
        app:layout_constraintBottom_toBottomOf="@+id/et_rtsp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view_password2" />

    <TextView
        android:id="@+id/tv_rtsp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="视频流："
        android:textColor="@color/color_1e1e1e"
        app:layout_constraintBottom_toBottomOf="@id/view_rtsp"
        app:layout_constraintStart_toStartOf="@+id/view_rtsp"
        app:layout_constraintTop_toTopOf="@+id/view_rtsp" />

    <EditText
        android:id="@+id/et_rtsp"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:background="@drawable/shape_eeeeeed_r40"
        android:hint="请输入视频流"
        android:maxLength="50"
        android:textColor="@color/color_1e1e1e"
        android:layout_marginStart="@dimen/dp_56"
        android:minLines="2"
        android:paddingStart="@dimen/dp_10"
        android:paddingEnd="@dimen/dp_10"
        app:layout_constraintEnd_toEndOf="@+id/view_rtsp"
        app:layout_constraintStart_toStartOf="@+id/view_rtsp"
        app:layout_constraintTop_toTopOf="@+id/view_rtsp"
        />
<!--   android:text="rtmp://58.200.131.2:1935/livetv/hunantv" -->

    <TextView
        android:id="@+id/tv_btn"
        android:layout_width="@dimen/dp_100"
        android:layout_height="@dimen/dp_38"
        android:layout_marginTop="@dimen/dp_10"
        android:background="@drawable/btn_0_1e1e1e_r40"
        android:gravity="center"
        android:text="确定"
        android:textColor="@color/color_1e1e1e"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view_rtsp" />

    <View
        android:layout_width="match_parent"
        app:layout_constraintTop_toBottomOf="@id/tv_btn"
        android:layout_height="@dimen/dp_10"/>

</androidx.constraintlayout.widget.ConstraintLayout>